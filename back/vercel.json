{
    "builds": [
      {
        "src": "api/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "front/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/server.js" },
      { "src": "/(.*)", "dest": "/front/$1" }
    ],
    "outputDirectory": "front",
    "cleanUrls": true
  }
  